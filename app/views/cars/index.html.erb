    <h1>Cars!</h1>
    <ul>
        <% @cars.each do |c| %>
            <li>
                <strong> Make is <%= c.make %> Year: <%= c.year %> Veh. trim <%= c.trim %> Mileage on veh. <%= c.mileage %> veh. transmission is <%= c.transmission %> Colour is <%= c.colour %> and asking price is $ <%= c.price %> ></strong>
                <%= link_to "More details!", cars_path(c) %>
                <%= link_to "Edit car details!", edit_car_path(c) %>
                 <%= link_to "Delete this car!", @car, method: :delete, data: { confirm: 'Are you sure you want to delete it?' } %>
            </li>
        <% end %>
    </ul>